    <Nullable>enable</Nullable>
    <PackageProjectUrl>https://github.com/marcosguedes01/efipay-api-integration</PackageProjectUrl>
    <Authors>Marcos Guedes</Authors>
    <FileVersion>1.0.3</FileVersion>
